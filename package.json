{
  "name": "beautyfile",
  "version": "0.1.0",
  "description": "Enumerate user folders in Linux with style!",
  "main": "index.js",
  "scripts": {
    "build-task:scss-compile": "node-sass-chokidar --source-map true --output-style compressed src/scss/ -o assets/kbl-css",
    "build-task:autoprefixer": "postcss assets/kbl-css/*.css --use autoprefixer -d assets/kbl-css",
    "sass:build": "npm-run-all -p build-task:*",
    "sass:watch": "chokidar 'src/**/*.scss' -c 'npm run sass:build'",
    "sass:importer": "node src/importer.js",
    "dev-scss": "npm-run-all -p sass:*",
    "dev-js": "nodemon src/index.js",
    "update": "git pull origin master",
    "production": "NODE_ENV=test node src/index.js"
  },
  "author": "Shelby McCowan",
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^10.0.1",
    "child-process-async": "^1.0.1",
    "chokidar-cli": "^2.1.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "dotenv-defaults": "^2.0.1",
    "express": "^4.17.1",
    "express-rate-limit": "^5.1.3",
    "node": "^14.13.0",
    "node-sass-chokidar": "^1.5.0",
    "node-sass-middleware": "^0.11.0",
    "npm-run-all": "^4.1.5",
    "nunjucks": "^3.2.2",
    "nunjucks-setasync": "^1.0.4",
    "postcss": "^8.1.1",
    "postcss-cli": "^8.1.0",
    "sass": "^1.27.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3",
    "winston": "^3.3.3"
  }
}
